<Application
    x:Class="NeuroMap_Exporter.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:NeuroMap_Exporter"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://NeuroMap Exporter/Icons.axaml" />
        <StyleInclude Source="avares://Neuromap Exporter/Styles.axaml" />
    </Application.Styles>
</Application>